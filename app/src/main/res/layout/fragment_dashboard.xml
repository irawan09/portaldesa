<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.laelektronik.user.portaldesa.Fragment.DashboardFragment">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:orientation="vertical">
        <LinearLayout
            android:id="@+id/test1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/test"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:layout_marginLeft="8dp"
                android:text="Kegiatan "/>

            <TextView
                android:id="@+id/penyelenggara"
                android:layout_width="wrap_content"
                android:layout_height="match_parent" />

        </LinearLayout>
        <LinearLayout
            android:id="@+id/test2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/test1"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="8dp">

            <TextView
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text="Nama Kegiatan"/>

            <TextView
                android:id="@+id/kegiatan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </LinearLayout>
        <LinearLayout
            android:id="@+id/test3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/test2"
            android:layout_marginLeft="8dp">

            <TextView
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text="Desa"/>

            <TextView
                android:id="@+id/desa"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/test4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/test3"
            android:layout_marginLeft="8dp">

            <TextView
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text="Kecamatan"/>

            <TextView
                android:id="@+id/kec"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/test5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/test4"
            android:layout_marginLeft="8dp">

            <TextView
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text="Kabupaten"/>

            <TextView
                android:id="@+id/kab"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/test6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/test5"
            android:layout_marginLeft="8dp">

            <TextView
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text="Provinsi"/>

            <TextView
                android:id="@+id/prov"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/test7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/test6"
            android:layout_marginLeft="8dp">

            <TextView
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text="Nilai Pagu"/>

            <TextView
                android:id="@+id/pagu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/test8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/test7"
            android:layout_marginLeft="8dp">

            <TextView
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text="Pemberi Tugas"/>

            <TextView
                android:id="@+id/pemberi_tugas"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/test9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/test8"
            android:layout_marginLeft="8dp">

            <TextView
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text="Subdit"/>

            <TextView
                android:id="@+id/subdit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/test10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/test9"
            android:layout_marginLeft="8dp">

            <TextView
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text="Penanggung Jawab"/>

            <TextView
                android:id="@+id/penanggung_jawab"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/test11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/test10"
            android:layout_marginLeft="8dp">

            <TextView
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text="Kontak Penanggung Jawab" />

            <TextView
                android:id="@+id/kontak_pj"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/test12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/test11"
            android:layout_marginLeft="8dp">

            <TextView
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text="Alamat Penanggung Jawab" />

            <TextView
                android:id="@+id/alamat_pj"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/test13"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/test12"
            android:layout_marginLeft="8dp">

            <TextView
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text="Nilai Kontrak" />

            <TextView
                android:id="@+id/nilai_kontrak"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/test14"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/test13"
            android:layout_marginLeft="8dp">

            <TextView
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text="Pelaksana" />

            <TextView
                android:id="@+id/pelaksana"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/test15"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/test16"
            android:layout_marginLeft="8dp">

            <TextView
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text="Pimpinan Pelaksana" />

            <TextView
                android:id="@+id/pimpinan_pelaksana"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/test16"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/test15"
            android:layout_marginLeft="8dp">

            <TextView
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text="Kontak Pelaksana" />

            <TextView
                android:id="@+id/kontak_pelaksana"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/test17"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/test16"
            android:layout_marginLeft="8dp">

            <TextView
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text="No. Kontrak" />

            <TextView
                android:id="@+id/kontrak"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/test18"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/test17"
            android:layout_marginLeft="8dp">

            <TextView
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text="Tanggal Kontrak" />

            <TextView
                android:id="@+id/tgl_kontrak"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/test19"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/test18"
            android:layout_marginLeft="8dp">

            <TextView
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text="Mulai Kerja" />

            <TextView
                android:id="@+id/mulai_kerja"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/test20"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/test19"
            android:layout_marginLeft="8dp">

            <TextView
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text="Selesai Kerja" />

            <TextView
                android:id="@+id/selesai_kerja"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/test21"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_below="@+id/test20"
            android:layout_marginLeft="8dp">

            <TextView
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:text="Lokasi" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Peta" />

        </LinearLayout>
    </LinearLayout>
</ScrollView>
s